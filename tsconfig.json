{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noEmitOnError": true,
    "strict": true,
    "allowJs": false,
    "checkJs": false,
    "jsx": "react",
    "noUnusedLocals": true,
    "outDir": "./build",
    "lib": ["es2015", "es2016", "es2017", "es6", "es7", "esnext.asynciterable", "dom"],
  },
  "exclude": [
    "node_modules",
    "android",
    "ios",
    "build",
    "__tests__"
  ]
}
